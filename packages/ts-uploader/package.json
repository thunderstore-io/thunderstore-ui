{
  "name": "@thunderstore/ts-uploader",
  "version": "0.1.0",
  "description": "Handles file uploads to Thunderstore",
  "repository": "https://github.com/thunderstore-io/thunderstore-ui/tree/master/packages/ts-uploader",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "types": "src/index.ts",
  "publishConfig": {
    "main": "dist/thunderstore-ts-uploader.umd.cjs",
    "module": "dist/thunderstore-ts-uploader.js",
    "types": "dist/index.d.ts",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/thunderstore-ts-uploader.js",
        "require": "./dist/thunderstore-ts-uploader.umd.cjs"
      },
      "./package.json": "./package.json"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "test": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@babel/runtime": "^7.25.6",
    "@thunderstore/graph-system": "^0.1.0",
    "@thunderstore/thunderstore-api": "^0.1.0",
    "@thunderstore/typed-event-emitter": "*",
    "crypto-js": "^4.2.0",
    "typescript": "^5.6.2"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.1.3",
    "@vitest/browser": "3.2.4",
    "playwright": "1.55.1",
    "vite": "^7.2.6",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "3.2.4"
  },
  "exports": {
    ".": "./src/index.ts",
    "./package.json": "./package.json"
  }
}
