  "type": "module",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "types": "src/index.ts",
  "style": "dist/cyberstorm.css",
  "publishConfig": {
    "main": "dist/thunderstore-cyberstorm.umd.cjs",
    "module": "dist/thunderstore-cyberstorm.js",
    "types": "dist/index.d.ts",
    "exports": {
      ".": {
        "import": "./dist/thunderstore-cyberstorm.js",
